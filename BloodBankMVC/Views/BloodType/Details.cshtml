@model BloodBank.Models.BloodTypeInfo

<h2>Blood Type Details: @Model.Type</h2>
<p><strong>Area:</strong> @Model.Area</p>
<p><strong>Stock Level:</strong> @Model.StockLevel</p>
<p><strong>Availability Status:</strong> @Model.AvailabilityStatus</p>
<p><strong>Last Updated:</strong> @Model.LastUpdated</p>

<a href="@Url.Action("Edit", new { Id = Model.Id, bloodTypeId = Model.Type })">Edit</a>

